Algoritmo "exercicio1_NomeEndereco"

Var

   Nome : caractere
   endereco: caractere
   


Inicio

     escreva("Digite seu nome: ")
     leia(nome)

     escreva("Digite seu endereço: ")
     leia(endereco)
     escreva("Seja bem vindo " + nome)
     escreva("Seu endereço é: " + endereco)

Fimalgoritmo